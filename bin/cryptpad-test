const { getCryptPadDrive } = require('../src/cryptpad/drive');

const driveUrl = 'https://freemium.cryptpad.fr/drive/#/2/drive/edit/5+9MFiXfG0w8T-g6GDQPefnC/';
const wsURL = 'wss://freemium.cryptpad.fr/cryptpad_websocket'

const rt = getCryptPadDrive(driveUrl, wsURL);
rt.proxy.on('ready', () => {
    console.log('READY');
    console.log(rt.proxy);
}).on('error', (info) => {
    console.error('ERROR');
    console.error(info);
});

